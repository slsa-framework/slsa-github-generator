{
  "name": "privacy-check",
  "version": "0.0.0",
  "private": true,
  "description": "GitHub action to protect private repositories.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "package": "ncc build --source-map",
    "lint": "eslint src/**/*.ts",
    "all": "npm ci && npm run format && npm run lint && npm run build && npm test && npm run package",
    "test": "jest"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "18.11.11",
    "@typescript-eslint/eslint-plugin": "5.45.1",
    "@typescript-eslint/parser": "5.45.1",
    "@vercel/ncc": "0.36.0",
    "eslint": "8.29.0",
    "eslint-plugin-github": "4.6.0",
    "prettier": "2.8.0",
    "ts-jest": "^29.0.3",
    "typescript": "4.9.3"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/github": "5.1.1"
  }
}
