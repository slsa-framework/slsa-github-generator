{
  "name": "tscommon",
  "version": "0.0.0",
  "private": true,
  "description": "Library for common utilities for TS Actions.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write 'src/**/*.ts'",
    "format-check": "prettier --check 'src/**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "npm pack",
    "all": "npm ci && npm run format && npm run lint && npm run build",
    "test": "jest"
  },
  "keywords": [
    "file",
    "node"
  ],
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/node": "20.17.16",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vercel/ncc": "0.38.3",
    "eslint": "8.57.1",
    "eslint-plugin-github": "4.10.2",
    "eslint-plugin-prettier": "5.2.3",
    "prettier": "3.4.2",
    "ts-jest": "29.2.5",
    "typescript": "5.7.3"
  }
}
