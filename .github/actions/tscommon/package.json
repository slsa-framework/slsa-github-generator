{
  "name": "tscommon",
  "version": "0.0.0",
  "private": true,
  "description": "Library for common utilities for TS Actions.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write 'src/**/*.ts'",
    "format-check": "prettier --check 'src/**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "npm pack",
    "all": "npm ci && npm run format && npm run lint && npm run build",
    "test": "jest"
  },
  "keywords": [
    "file",
    "node"
  ],
  "devDependencies": {
    "@types/jest": "29.5.5",
    "@types/node": "18.18.1",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vercel/ncc": "0.38.0",
    "eslint": "8.50.0",
    "eslint-plugin-github": "4.10.1",
    "eslint-plugin-prettier": "5.0.0",
    "prettier": "3.0.3",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  }
}
