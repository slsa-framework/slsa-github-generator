{
  "name": "tscommon",
  "version": "0.0.0",
  "private": true,
  "description": "Library for common utilities for TS Actions.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write 'src/**/*.ts'",
    "format-check": "prettier --check 'src/**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "npm pack",
    "all": "npm ci && npm run format && npm run lint && npm run build",
    "test": "jest"
  },
  "keywords": [
    "file",
    "node"
  ],
  "devDependencies": {
    "@types/jest": "29.5.3",
    "@types/node": "18.17.4",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "@vercel/ncc": "0.36.1",
    "eslint": "8.46.0",
    "eslint-plugin-github": "4.9.2",
    "eslint-plugin-prettier": "5.0.0",
    "prettier": "3.0.1",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6"
  }
}
